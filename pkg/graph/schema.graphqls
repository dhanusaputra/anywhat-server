type Query {
  anythings: [Anything!]!
}

type Anything {
  id: ID!
  name: String!
  description: String!
  user: User!
  createdAt: Timestamp!
  updatedAt: Timestamp!
}

type User {
  id: ID!
  username: String!
  password: String!
  createdAt: Timestamp!
  lastLoginAt: Timestamp!
}

scalar Timestamp